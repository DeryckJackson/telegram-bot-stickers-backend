{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "semi": 0,
    "indent": [
      "warn",
      2
    ],
    "no-console": "warn",
    "no-debugger": "warn",
    "no-unused-vars": "warn"
  }
}
